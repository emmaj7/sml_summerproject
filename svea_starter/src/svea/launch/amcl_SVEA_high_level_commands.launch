<launch>


  <!--open serial connection for controlling SVEA-->
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
      <param name="port" value="/dev/ttyACM0"/>
      <param name="baud" value="115200"/>
  </node>

  <!-- start lidar -->
  <include file="$(find rplidar_ros)/launch/rplidar.launch"/>

  <!-- transform between base_link and laser scanner -->
  <node pkg="tf" type="static_transform_publisher" name="laser" args=" 0.1 0 0.2 0 0 0 1  /base_link /laser  1"/>

  <!--start qualisys odometry-->
  <!-- <include file="$(find qualisys)/launch/qualisys.launch"/>
  <include file="$(find qualisys)/launch/qualisys_odom.launch">
      <arg name="model" value="SVEA5"/>
  </include> -->

  <!-- start amcl localization -->
  <include file= "$(find navigation)/amcl/examples/amcl_diff.launch"/>

  <arg name= "my_args"/>
  <node pkg="svea" type="amcl_SVEA_high_level_commands.py" name="amcl_SVEA_high_level_commands" args="$(arg my_args)" output="screen"/>

</launch>
